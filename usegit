git init

##################local####################
git add <filename>
git add *
git commit -m "msg for this"

###rollback###
git log
git reset --hard HEAD~1,2,3

#################remote###################
git remote add origin https://github.com/lvluo3/mgr.git
git push origin master

###rollback###
git fetch origin
git reset --hard origin/master


####create branch####
git branch xxx
git checkout	xxx

###look remote branch###
git branch -a
